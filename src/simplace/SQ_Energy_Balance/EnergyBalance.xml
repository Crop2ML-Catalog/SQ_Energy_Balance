<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//SIMPLACE/DTD GRP 1.0//EN" "http://simplace.net/dtd/GroupComponent.dtd">
<configuration class="net.simplace.sim.components.SQ_Energy_Balance">
    <simgroup>
        <simcomponent id="NetRadiation" class="net.simplace.sim.components.SQ_Energy_Balance.NetRadiation">
            <input id="minTair" source="EnergyBalance.minTair"/>
            <input id="maxTair" source="EnergyBalance.maxTair"/>
            <input id="albedoCoefficient" source="EnergyBalance.albedoCoefficient"/>
            <input id="stefanBoltzman" source="EnergyBalance.stefanBoltzman"/>
            <input id="elevation" source="EnergyBalance.elevation"/>
            <input id="solarRadiation" source="EnergyBalance.solarRadiation"/>
            <input id="vaporPressure" source="EnergyBalance.vaporPressure"/>
            <input id="extraSolarRadiation" source="EnergyBalance.extraSolarRadiation"/>
            <output id="netRadiation" destination="EnergyBalance.netRadiation"/>
            <output id="netOutGoingLongWaveRadiation" destination="EnergyBalance.netOutGoingLongWaveRadiation"/></simcomponent>
        <simcomponent id="Conductance" class="net.simplace.sim.components.SQ_Energy_Balance.Conductance">
            <input id="vonKarman" source="EnergyBalance.vonKarman"/>
            <input id="heightWeatherMeasurements" source="EnergyBalance.heightWeatherMeasurements"/>
            <input id="zm" source="EnergyBalance.zm"/>
            <input id="d" source="EnergyBalance.d"/>
            <input id="zh" source="EnergyBalance.zh"/>
            <input id="plantHeight" source="EnergyBalance.plantHeight"/>
            <input id="wind" source="EnergyBalance.wind"/>
            <output id="conductance" destination="EnergyBalance.conductance"/></simcomponent>
        <simcomponent id="DiffusionLimitedEvaporation" class="net.simplace.sim.components.SQ_Energy_Balance.DiffusionLimitedEvaporation">
            <input id="deficitOnTopLayers" source="EnergyBalance.deficitOnTopLayers"/>
            <input id="soilDiffusionConstant" source="EnergyBalance.soilDiffusionConstant"/>
            <output id="diffusionLimitedEvaporation" destination="EnergyBalance.diffusionLimitedEvaporation"/></simcomponent>
        <simcomponent id="NetRadiationEquivalentEvaporation" class="net.simplace.sim.components.SQ_Energy_Balance.NetRadiationEquivalentEvaporation">
            <input id="lambdaV" source="EnergyBalance.lambdaV"/>
            <input id="netRadiation" source="NetRadiation.netRadiation"/>
            <output id="netRadiationEquivalentEvaporation" destination="EnergyBalance.netRadiationEquivalentEvaporation"/></simcomponent>
        <simcomponent id="PriestlyTaylor" class="net.simplace.sim.components.SQ_Energy_Balance.PriestlyTaylor">
            <input id="hslope" source="EnergyBalance.hslope"/>
            <input id="psychrometricConstant" source="EnergyBalance.psychrometricConstant"/>
            <input id="Alpha" source="EnergyBalance.Alpha"/>
            <input id="netRadiationEquivalentEvaporation" source="NetRadiationEquivalentEvaporation.netRadiationEquivalentEvaporation"/>
            <output id="evapoTranspirationPriestlyTaylor" destination="EnergyBalance.evapoTranspirationPriestlyTaylor"/></simcomponent>
        <simcomponent id="PtSoil" class="net.simplace.sim.components.SQ_Energy_Balance.PtSoil">
            <input id="Alpha" source="EnergyBalance.Alpha"/>
            <input id="tau" source="EnergyBalance.tau"/>
            <input id="tauAlpha" source="EnergyBalance.tauAlpha"/>
            <input id="evapoTranspirationPriestlyTaylor" source="PriestlyTaylor.evapoTranspirationPriestlyTaylor"/>
            <output id="energyLimitedEvaporation" destination="EnergyBalance.energyLimitedEvaporation"/></simcomponent>
        <simcomponent id="Penman" class="net.simplace.sim.components.SQ_Energy_Balance.Penman">
            <input id="hslope" source="EnergyBalance.hslope"/>
            <input id="VPDair" source="EnergyBalance.VPDair"/>
            <input id="psychrometricConstant" source="EnergyBalance.psychrometricConstant"/>
            <input id="Alpha" source="EnergyBalance.Alpha"/>
            <input id="lambdaV" source="EnergyBalance.lambdaV"/>
            <input id="rhoDensityAir" source="EnergyBalance.rhoDensityAir"/>
            <input id="specificHeatCapacityAir" source="EnergyBalance.specificHeatCapacityAir"/>
            <input id="evapoTranspirationPriestlyTaylor" source="PriestlyTaylor.evapoTranspirationPriestlyTaylor"/>
            <input id="conductance" source="Conductance.conductance"/>
            <output id="evapoTranspirationPenman" destination="EnergyBalance.evapoTranspirationPenman"/></simcomponent>
        <simcomponent id="SoilEvaporation" class="net.simplace.sim.components.SQ_Energy_Balance.SoilEvaporation">
            <input id="diffusionLimitedEvaporation" source="DiffusionLimitedEvaporation.diffusionLimitedEvaporation"/>
            <input id="energyLimitedEvaporation" source="PtSoil.energyLimitedEvaporation"/>
            <output id="soilEvaporation" destination="EnergyBalance.soilEvaporation"/></simcomponent>
        <simcomponent id="EvapoTranspiration" class="net.simplace.sim.components.SQ_Energy_Balance.EvapoTranspiration">
            <input id="isWindVpDefined" source="EnergyBalance.isWindVpDefined"/>
            <input id="evapoTranspirationPriestlyTaylor" source="PriestlyTaylor.evapoTranspirationPriestlyTaylor"/>
            <input id="evapoTranspirationPenman" source="Penman.evapoTranspirationPenman"/>
            <output id="evapoTranspiration" destination="EnergyBalance.evapoTranspiration"/></simcomponent>
        <simcomponent id="SoilHeatFlux" class="net.simplace.sim.components.SQ_Energy_Balance.SoilHeatFlux">
            <input id="tau" source="EnergyBalance.tau"/>
            <input id="netRadiationEquivalentEvaporation" source="NetRadiationEquivalentEvaporation.netRadiationEquivalentEvaporation"/>
            <input id="soilEvaporation" source="SoilEvaporation.soilEvaporation"/>
            <output id="soilHeatFlux" destination="EnergyBalance.soilHeatFlux"/></simcomponent>
        <simcomponent id="PotentialTranspiration" class="net.simplace.sim.components.SQ_Energy_Balance.PotentialTranspiration">
            <input id="tau" source="EnergyBalance.tau"/>
            <input id="evapoTranspiration" source="EvapoTranspiration.evapoTranspiration"/>
            <output id="potentialTranspiration" destination="EnergyBalance.potentialTranspiration"/></simcomponent>
        <simcomponent id="CropHeatFlux" class="net.simplace.sim.components.SQ_Energy_Balance.CropHeatFlux">
            <input id="netRadiationEquivalentEvaporation" source="NetRadiationEquivalentEvaporation.netRadiationEquivalentEvaporation"/>
            <input id="potentialTranspiration" source="PotentialTranspiration.potentialTranspiration"/>
            <input id="soilHeatFlux" source="SoilHeatFlux.soilHeatFlux"/>
            <output id="cropHeatFlux" destination="EnergyBalance.cropHeatFlux"/></simcomponent>
        <simcomponent id="CanopyTemperature" class="net.simplace.sim.components.SQ_Energy_Balance.CanopyTemperature">
            <input id="minTair" source="EnergyBalance.minTair"/>
            <input id="maxTair" source="EnergyBalance.maxTair"/>
            <input id="lambdaV" source="EnergyBalance.lambdaV"/>
            <input id="rhoDensityAir" source="EnergyBalance.rhoDensityAir"/>
            <input id="specificHeatCapacityAir" source="EnergyBalance.specificHeatCapacityAir"/>
            <input id="conductance" source="Conductance.conductance"/>
            <input id="cropHeatFlux" source="CropHeatFlux.cropHeatFlux"/>
            <output id="minCanopyTemperature" destination="EnergyBalance.minCanopyTemperature"/>
            <output id="maxCanopyTemperature" destination="EnergyBalance.maxCanopyTemperature"/></simcomponent></simgroup></configuration>